<thlogic>
    <attr sel="#imgContainer" th:fragment="imageContainerFrag"/>
    <attr sel="#imgContainer"
          th:each="__${T(com.maxpapers.constants.Attribute).PHOTO}__ : ${__${T(com.maxpapers.constants.Attribute).SEARCH_RESULTS}__}"/>
    <attr sel="#imgContainer" th:id="|imgContainer${__${T(com.maxpapers.constants.Attribute).PHOTO}__.id}|"/>
    <attr sel="#imgContainer" th:action="|@{__${T(com.maxpapers.constants.Mapping).VIEW}__}?id=${__${T(com.maxpapers.constants.Attribute).PHOTO}__.id}|"/>
    <attr sel="#wallpaper"
          th:src="|data:image/jpg;base64,${__${T(com.maxpapers.constants.Attribute).PHOTO}__.encodedString}|"/>
    <attr sel="#wallpaper" th:id="|searchImg${__${T(com.maxpapers.constants.Attribute).PHOTO}__.id}|"/>
</thlogic>